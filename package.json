{
  "name": "@easyai/cli",
  "version": "2.0.11",
  "description": "AI development tool with CLI and interactive dashboard - build, test, and manage AI prompts across multiple models",
  "main": "dist/cli.js",
  "bin": {
    "easyai": "./dist/cli.js"
  },
  "scripts": {
    "build": "npm run build:cli && npm run build:frontend",
    "build:cli": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "dev": "tsx watch src/cli.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "node backend-server.js",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build:cli",
    "test": "echo \"Tests coming soon\" && exit 0"
  },
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "ai",
    "cli",
    "development",
    "openai",
    "anthropic",
    "llm",
    "prompt",
    "chatgpt",
    "claude",
    "interactive",
    "playground"
  ],
  "author": "EasyAI <support@easy-ai.dev>",
  "license": "MIT",
  "homepage": "https://easy-ai.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/easyai/cli.git"
  },
  "bugs": {
    "url": "https://github.com/easyai/cli/issues"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.0",
    "openai": "^4.104.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.0.0",
    "tsx": "^3.12.0",
    "typescript": "^5.0.0"
  }
}
